{
   "name": "@bikeshop/ui-web",
   "version": "2.2.4",
   "private": true,
   "dependencies": {
      "@bikeshop/core": "^2.2.4",
      "@bikeshop/network": "^2.2.4",
      "@bikeshop/storage": "^2.2.4",
      "react": "^17.0.2",
      "react-dom": "^17.0.2"
   },
   "scripts": {
      "prestart": "yarn build:styles",
      "start": "react-scripts start",
      "prebuild": "NODE_ENV=production && yarn build:styles",
      "eject": "react-scripts eject",
      "clean": "rimraf build",
      "build:styles": "postcss src/css/tailwind.src.css -o src/css/tailwind.css"
   },
   "eslintConfig": {
